# Copy this file to .env and replace placeholder values with your Vision service-account details.
TYPE=service_account
PROJECT_ID=your-project-id
PRIVATE_KEY_ID=your-private-key-id
PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\\nYOUR-PRIVATE-KEY\\n-----END PRIVATE KEY-----\\n"
CLIENT_EMAIL=service-account@your-project-id.iam.gserviceaccount.com
CLIENT_ID=your-client-id
AUTH_URI=https://accounts.google.com/o/oauth2/auth
TOKEN_URI=https://oauth2.googleapis.com/token
AUTH_PROVIDER_CERT=https://www.googleapis.com/oauth2/v1/certs
CLIENT_CERT=https://www.googleapis.com/robot/v1/metadata/x509/service-account%40your-project-id.iam.gserviceaccount.com
UNIVERSE_DOMAIN=googleapis.com

# Server configuration
PORT=3000
API_KEY=change-me
ALLOWED_ORIGINS=http://localhost:3000
MAX_IMAGE_BYTES=5242880
ALLOWED_MIME_TYPES=image/png,image/jpeg,image/jpg
VISION_TIMEOUT_MS=8000
VISION_MAX_RETRIES=1
